<h1>Products home</h1>
<table>
    <tr> 
        <th>Title</th>
        <th>Description</th> 
        <th>Price</th> 
        <th>Code</th> 
        <th>Stock</th> 
        <th>Category</th> 
        <th>Status</th> 
    </tr>
    <tbody>
        {{#each products}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.title}}</td>
                <td>{{this.description}}</td>
                <td>{{this.code}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
                <td>{{this.status}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>

<script src="/socket.io/socket.io.js"></script>
<script src="/js/home.js"></script>